{
  "name": "parking-garage-api",
  "version": "1.0.0",
  "description": "RESTful API for managing parking garage operations",
  "main": "dist/src/server.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/src/server.js",
    "start:prod": "node dist/src/server.js",
    "dev": "ts-node --transpile-only src/server.ts",
    "dev:watch": "nodemon --exec ts-node src/server.ts",
    "test": "NODE_ENV=test jest --verbose",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:integration": "NODE_ENV=test jest --testPathPattern=tests/integration --verbose",
    "test:unit": "NODE_ENV=test jest --testPathPattern=tests/unit --verbose",
    "test:database": "NODE_ENV=test jest --testPathPattern=tests/database --verbose",
    "test:performance": "NODE_ENV=test jest --testPathPattern=tests/performance --verbose",
    "test:setup": "NODE_ENV=test node tests/helpers/setup-database.js",
    "lint": "eslint src/ --ext .ts,.js",
    "lint:fix": "eslint src/ --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js}\"",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:reset": "prisma migrate reset",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "db:migrate-data": "ts-node scripts/migrate-data.ts",
    "db:rollback": "ts-node scripts/rollback-migration.ts",
    "db:validate": "ts-node scripts/migrate-data.ts --validate-only",
    "db:backup": "ts-node -e \"import {DataBackupUtility} from './src/utils/data-backup'; new DataBackupUtility().createBackup('manual-backup', {includeMemoryStore: true, includeSQLiteDB: true})\""
  },
  "keywords": [
    "api",
    "parking",
    "garage",
    "express",
    "nodejs"
  ],
  "author": "Parking Garage Management System",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "^1.55.0",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/node": "^10.8.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.1",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-status-monitor": "^1.3.4",
    "express-validator": "^7.2.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.6",
    "pino": "^9.9.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "redis": "^5.8.2",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yamljs": "^0.3.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^8.45.0",
    "@faker-js/faker": "^8.4.1",
    "@prisma/client": "^5.20.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "prisma": "^6.15.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
